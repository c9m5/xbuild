#!/bin/sh

if [ ! "$xbuild_prefix" ] ; then
    xbuild_prefix=`realpath $0`
    xbuild_prefix="${xbuild_prefix%%/bin/xbuild-install}"; export xbuild_prefix
fi

. ${xbuild_prefix}/lib/xbuild/config.sh

xb_install_usage() {
echo << __EOF__
    "USAGE: xbuild install"
__EOF__
}

xb_install_help() {
}

xb_dialog_install() {
}



